{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@components/*": ["./src/components/*"],
          "@api/*": ["./src/api/*"],
          "@models/*": ["./src/models/*"],
          "@lib/*": ["./src/lib/*"],
          "@hooks/*": ["./src/hooks/*"],
          "@services/*": ["./src/services/*"],
          "@constants": ["./src/constants.ts"],
          "@context/*": ["./src/context/*"],
          "@graphql/*": ["./src/graphql/*"],
          "@data/*": ["./src/data/*"],
          "@typeDefs/*": ["./src/types/*"],
          "@generated/*": ["./src/generated/*"],
          "@public/*": ["./public/*"],
          "@utils/*": ["./src/utils/*"],
          "@app/*": ["./src/app/*"],
          "@assets/*": ["./src/assets/*"],
          "@helpers/*": ["./src/helpers/*"]
        }
      }
    ],
    ["styled-components", { "ssr": true }]
  ]
}
